{
	"name": "AlkerniaLegends",
	"description": "2D Pixel Platformer Game with Game Feel and Juice",
	"version": "8.3",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"offline_enabled": true
}